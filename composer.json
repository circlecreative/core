{
    "name": "neo/core",
    "description": "Neo core",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nur Wachid",
            "email": "nur@wach.id"
        }
    ],
    "autoload": {
        "psr-4": {
            "Neo\\Core\\": "src/"
        }
