        "illuminate/database": "^5.6|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0",
        "intervention/imagecache": "^2.5",
        "jenssegers/agent": "^2.6",
        "kalnoy/nestedset": "^6.0",
        "pragmarx/google2fa": "^8.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-activitylog": "^4.3",
        "spatie/laravel-backup": "^6.16",
        "spatie/laravel-sluggable": "^3.1",
        "league/fractal": "^0.19.2",
        "turahe/laravel-installer": "dev-master",
        "turahe/seotools": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 | ^8.5 | ^9.3",
        "php-coveralls/php-coveralls": "^2.1",
        "orchestra/testbench": "3.*| ^4.0| ^5.0 | ^6.0",
        "friendsofphp/php-cs-fixer": "^3.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neo\\Core\\Providers\\NeoCoreServiceProvider"
            ]
        }
    }
}
